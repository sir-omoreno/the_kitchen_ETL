    "links = browser.find_by_text('Get Recipe')\n",
    "\n",
    "for link in links:\n",
    "    link.click()\n",
    "    \n",
    "    html=browser.html\n",
    "    soup=BeautifulSoup(html, 'html.parser')\n",
    "    \n",
    "    browser.back()\n",
    "    time.sleep(2)\n",
    "    next\n",
